  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

/* ==================
        折叠面板collapse
 ==================== */

.yu-collapse-item {
  position: relative;
}

.yu-collapse-item::before {
  content: " ";
  position: absolute;
  top: 0;
  right: 0;
  height: 1rpx;
  border-top: 1rpx solid #e5e5e5;
  color: #e5e5e5;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
  left: 30rpx;
  z-index: 2;
}

.yu-collapses .yu-collapse-item:first-child::before {
  display: none;
}

.yu-collapse {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  background-color: #fff;
  padding: 24rpx 30rpx;
  font-size: 32rpx;
}

.yu-collapse-hd [class*="icon-"] {
  font-size: 40rpx;
  color: #999;
  padding-right: 6rpx;
  width: 18px;
}

.yu-collapse-bd {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.yu-collapse-ft [class*="icon-"] {
  font-size: 40rpx;
  color: #999;
  padding-right: 6rpx;
  width: 18px;
}

.yu-collapse-content {
  padding: 20rpx 30rpx;
}

/* ==================
      进度条tree
 ==================== */

.yu-tree {
  padding: 20rpx 0;
  background: #fff;
}

.yu-tree-header {
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  color: #555;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}

.yu-tree-title {
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
}

.yu-tree-mark {
  font-size: 26rpx;
  padding-left: 10rpx;
}

.yu-tree>.yu-tree-item>.yu-tree-header {
  height: 84rpx;
  line-height: 84rpx;
}

.yu-tree .yu-tree-item {
  background: #fff;
  padding-left: 40rpx;
}

.yu-tree>.yu-tree-item {
  padding: 0 30rpx;
}

/* ==================
      进度条progress
 ==================== */

.yu-progress {
  overflow: hidden;
  height: 18rpx;
  background-color: #ebebeb;
  display: inline-flex;
  align-items: center;
  width: 100%;
  color: #fff;
}

.yu-progress-size-mini {
  height: 6rpx;
}

.yu-progress-size-sm {
  height: 12rpx;
}

.yu-progress-size-md {
  height: 24rpx;
}

.yu-progress-size-lg {
  height: 30rpx;
}

.yu-progress-item {
  width: 0;
  height: 100%;
  align-items: center;
  display: flex;
  justify-items: flex-end;
  justify-content: space-around;
  font-size: 20rpx;
  color: #fff;
  transition: width 0.6s ease;
}

/* ==================
      form表单
 ==================== */

.yu-form {
  background-color: #fff;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  padding: 0 30rpx;
  font-size: 32rpx;
}

.yu-form::before {
  content: " ";
  position: absolute;
  bottom: 0;
  right: 0;
  height: 1rpx;
  border-bottom: 1rpx solid #e5e5e5;
  color: #e5e5e5;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
  left: 30rpx;
  z-index: 99;
}

.yu-check {
  display: none;
}

.yu-switch {
  padding: 12rpx 0;
}

.yu-label {
  width: 180rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.yu-input {
  height: 88rpx;
  min-height: 88rpx;
  line-height: 88rpx;
}

.yu-textarea {
  padding: 30rpx 0;
  display: block;
  width: 100%;
  height: 120rpx;
}

.yu-textarea-counter {
  color: #b2b2b2;
  text-align: right;
  padding-bottom: 30rpx;
}

.yu-check-icon {
  vertical-align: middle;
  padding: 10rpx;
}

.yu-form-bd {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
}

.yu-form-title {
  padding: 24rpx 0;
  line-height: 1.4;
}

.yu-vcode-btn button {
  font-size: 28rpx;
  display: block;
  width: 200rpx;
  padding: 0;
}

.yu-vcode-img {
  width: 200rpx;
  height: 72rpx;
  border-radius: 10rpx;
}

.yu-form-ft {
  color: #999;
  position: relative;
}

.yu-form-ft.yu-form-icon {
  padding-right: 30rpx;
}

.yu-form-ft.yu-form-icon::after {
  content: '';
  display: inline-block;
  width: 8px;
  height: 8px;
  border-top: 1rpx solid #999;
  border-right: 1rpx solid #999;
  transform: rotate(45deg);
  -webkit-transform: rotate(45deg);
  vertical-align: middle;
  position: absolute;
  top: 50%;
  right: 2px;
  margin-top: -4px;
}

/* ==================
        轮播swiper
 ==================== */

.yu-swiper {
  min-height: 360rpx;
}

.yu-swiper-img {
  width: 100%;
}

.yu-swiper-card {
  min-height: 360rpx;
}

/* .yu-swiper-card swiper-item{
  width: 80%!important;
  left: 10%;
}
.yu-swiper-card .yu-swiper-img{
  width: 96%;
  height: 100%;
  margin:0 2%;
  border-radius: 6px;
} */

.yu-swiper-card swiper-item {
  width: 80% !important;
  left: 10%;
  padding: 30rpx 0 60rpx;
  box-sizing: border-box;
}

.yu-swiper-card .yu-swiper-img {
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
  transform: scale(0.9);
  transition: all 0.2s ease-in 0s;
}

.yu-swiper-card swiper-item.current  .yu-swiper-img {
  transform: none;
  transition: all 0.2s ease-in 0s;
}

/* ==================
      时间轴timeline
 ==================== */

.yu-timeline {
  display: block;
  background: #fff;
}

.yu-timeline .yu-time {
  width: 120rpx;
  text-align: center;
  padding: 20rpx 0;
  font-size: 26rpx;
  color: #888;
  display: block;
}

.yu-timeline .yu-item {
  padding: 30rpx 30rpx 30rpx 120rpx;
  position: relative;
  display: block;
  z-index: 0;
}

.yu-timeline .yu-item:not([class*="yu-text-"]) {
  color: #ccc;
}

.yu-timeline .yu-item::after {
  content: "";
  display: block;
  position: absolute;
  width: 1rpx;
  background: #ddd;
  left: 60rpx;
  height: 100%;
  top: 0;
  z-index: 8;
}

.yu-timeline .yu-item::before {
  font-family: "iconfont";
  display: block;
  position: absolute;
  left: 36rpx;
  top: 36rpx;
  z-index: 9;
  background: #fff;
  width: 50rpx;
  height: 50rpx;
  text-align: center;
  border: none;
  line-height: 50rpx;
}

.yu-timeline .yu-item:not([class*="icon-"])::before {
  content: "\e6ea";
}

.yu-timeline .yu-item[class*="icon"]::before {
  background: #fff;
  width: 50rpx;
  height: 50rpx;
  text-align: center;
  border: none;
  line-height: 50rpx;
  left: 36rpx;
}

.yu-timeline .yu-timeline-content {
  padding: 20rpx 30rpx;
  border-radius: 10rpx;
  display: block;
  line-height: 1.4;
  text-align: justify;
}

.yu-timeline .yu-timeline-content:not([class*="bg-"]) {
  background: #f2f2f2;
  color: #333;
}

/* ==================
      卡片card
 ==================== */

.yu-card {
  background: #fff;
  padding: 30rpx;
}

.yu-card-text {
  text-align: justify;
  padding: 20rpx 0;
}

.yu-card-img {
  border-radius: 10rpx;
}

.yu-card-img image {
  width: 100%;
  border-radius: 10rpx;
}

.yu-card-header {
  width: 96rpx;
  height: 96rpx;
  margin-right: 10rpx;
}

.yu-card-header image {
  width: 100%;
  border-radius: 48rpx;
}

.yu-card-nickname {
  color: #333;
  font-size: 32rpx;
  padding: 6rpx 0;
}

.yu-card-info {
  color: #999;
  font-size: 24rpx;
  line-height: 36rpx;
}

.yu-card-delete {
  color: #587ab3;
  font-size: 28rpx;
  line-height: 36rpx;
}

.yu-card-operate {
  font-size: 28rpx;
  color: #999;
  text-align: right;
  line-height: 36rpx;
}

.yu-card-operate .item {
  display: inline;
  padding-left: 8rpx;
}

.yu-card-out {
  /* 卡片式 */
  margin: 0 30rpx;
  padding: 0 0 30rpx;
  border-radius: 10rpx;
}

.yu-card-out .yu-card-content {
  padding: 0 30rpx;
}

.yu-card-out .yu-card-img image {
  border-radius: 10rpx 10rpx 0 0;
}

.yu-card-dynamic-out {
  /* 动态卡片 */
  margin: 0 30rpx;
  border-radius: 10rpx;
}

.yu-card-goods {
  padding: 10rpx 20rpx;
  background: #fff;
}

.yu-card-goods .yu-card {
  padding: 0;
}

.yu-card-price {
  color: #f00;
  font-size: 32rpx;
  line-height: 36rpx;
  text-align: left;
}
.yu-card-goods-list{
  position: relative;
}
.yu-card-goods-list::after{
  content: " ";
  position: absolute;
  top: 0;
  right: 0;
  height: 1rpx;
  border-top: 1rpx solid #e5e5e5;
  color: #e5e5e5;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
  left: 30rpx;
  z-index: 2;
}
.yu-card-goods-list .yu-card-img{
  width:240rpx;
  padding-right:20rpx;
}
.yu-card-goods-list .yu-card-text{
  text-align: left;
  padding:0;
  margin:16rpx 0;
}

/* ==================
      对话框dialog
 ==================== */

.yu-dialog-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
  background: #000;
  opacity: 0.4;
  filter: alpha(opacity=40);
  overflow: hidden;
}

.yu-dialog-box {
  position: fixed;
  top: 280rpx;
  left: 0;
  right: 0;
  z-index: 1000;
  background: #fff;
  margin: 0 80rpx;
  border-radius: 10rpx;
}

.yu-dialog-title {
  font-size: 36rpx;
  padding-top: 30rpx;
  text-align: center;
}

.yu-dialog-content {
  padding: 30rpx 40rpx;
  color: #999;
  text-align: justify;
}

.yu-dialog-handle {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  text-align: center;
  line-height: 88rpx;
  font-size: 32rpx;
  border-top: 1rpx solid #f2f2f2;
  box-sizing: border-box;
}

.yu-dialog-handle .cancel {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  box-sizing: border-box;
  border-right: 1rpx solid #f2f2f2;
  border-radius: 0 0 10rpx 10rpx;
}

.yu-dialog-handle .cancel:active {
  background: #eee;
}

.yu-dialog-handle .comfirm {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  border-radius: 0 0 10rpx 10rpx;
}

.yu-dialog-handle .comfirm:active {
  background: #eee;
}

/* ==================
      对话框toast
 ==================== */

.yu-toast-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
  background: #000;
  opacity: 0.01;
  filter: alpha(opacity=1);
  overflow: hidden;
}

.yu-toast-box {
  position: fixed;
  top: 280rpx;
  left: 0;
  right: 0;
  z-index: 1000;
  background: rgba(0, 0, 0, 0.75);
  color: #fff;
  /* opacity: 0.75;
  filter: alpha(opacity=75); */
  margin: 0 25%;
  border-radius: 10rpx;
}

.yu-toast-content {
  padding: 30rpx 40rpx;
  text-align: center;
}

.yu-toast-content [class*="icon-"] {
  font-size: 100rpx;
  padding-bottom: 12rpx;
}

.yu-toast-content text {
  font-size: 28rpx;
}

/* ==================
      选项卡tab
 ==================== */

.yu-tab {
  position: relative;
  height: 100%;
}

.yu-tabbar {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  position: absolute;
  z-index: 1000;
  top: 0;
  width: 100%;
  border-bottom: 1rpx solid #ccc;
}

.yu-tabbar-item {
  position: relative;
  display: block;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
}

.yu-tabbar-item.yu-tabbar-item-on {
  color: #3565ee;
}

.yu-tabbar-title {
  display: inline-block;
  font-size: 30rpx;
  max-width: 84px;
  width: auto;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  word-wrap: normal;
}

.yu-tabbar-slider {
  position: absolute;
  content: " ";
  left: 0;
  bottom: 0;
  width: 84px;
  height: 3px;
  background-color: #3565ee;
  -webkit-transition: -webkit-transform 0.3s;
  transition: -webkit-transform 0.3s;
  transition: transform 0.3s;
  transition: transform 0.3s, -webkit-transform 0.3s;
}

.yu-tabbar-panel {
  box-sizing: border-box;
  height: 100%;
  padding-top: 100rpx;
  overflow: auto;
  -webkit-overflow-scrolling: touch;
}

.yu-tabbar-content {
  padding: 30rpx;
}

.yu-scroll-nav {
  white-space: nowrap;
}

.yu-scroll-item {
  height: 90rpx;
  display: inline-block;
  line-height: 90rpx;
  margin: 0 10rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
}

.yu-scroll-item.current {
  border-bottom: 6rpx solid #3565ee;
}

.yu-scroll-content {
  padding: 30rpx;
}

/* ==================
      表格table
 ==================== */

.yu-table-box {
  white-space: nowrap;
}

.yu-table {
  position: relative;
}

.yu-table-tr {
  width: 200rpx;
  display: inline-block;
  overflow: hidden;
}

.yu-table-fixed {
  width: 200rpx;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 99;
  box-shadow: 2rpx -8rpx 30rpx #ddd;
}

.yu-table-th {
  background: rgb(237, 248, 255);
  font-size: 28rpx;
  font-weight: bold;
  height: 80rpx;
  padding: 0 12rpx;
  color: #333;
  line-height: 80rpx;
  border-bottom: 1rpx solid #ccc;
  text-align: center;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}

.yu-table-td {
  background: #fff;
  font-size: 28rpx;
  height: 80rpx;
  padding: 0 12rpx;
  color: #333;
  line-height: 80rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  border-bottom: 1rpx solid #ccc;
}

/* ==================
      底部footer
 ==================== */

.yu-footer {
  color: #808080;
  text-align: center;
  height: 60rpx;
  line-height: 60rpx;
}

.yu-footer-fixd {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
}

.yu-footer-btn {
  color: #333;
  font-size: 28rpx;
  margin: 0 30rpx;
  border-radius: 10rpx;
  box-sizing: border-box;
}

.yu-footer-btn-border {
  color: #333;
  font-size: 28rpx;
  box-sizing: border-box;
  margin: 0 30rpx;
  border-radius: 10rpx;
  border: 1rpx solid #e6e6e6;
}

.yu-footer-btn:active {
  background: #e6e6e6;
}

.yu-footer-btn-border:active {
  background: #f6f7f8;
}

.yu-footer-link {
  color: #586c94;
  font-size: 28rpx;
}

.yu-footer-text {
  color: #999;
  font-size: 24rpx;
}

/* ==================
      无数据null
 ==================== */

.yu-null {
  text-align: center;
  font-size: 28rpx;
}

.yu-null-icon {
  font-size: 160rpx;
  color: #bec5cd;
}

.yu-null-img image {
  width: 40%;
}

.yu-null-text {
  padding-top: 30rpx;
  color: #999;
  font-size: 30rpx;
}

.yu-null-btn {
  margin-top: 30rpx;
  display: inline-block;
  padding: 0 30rpx;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 10rpx;
  box-sizing:border-box;
}

.yu-null [class*="yu-bg-"] {
  color: #fff;
}