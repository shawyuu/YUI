/* ==================
        flex布局
 ==================== */

.yu-flex {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}

.yu-flex-item {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
}

.yu-flex-item-double {
  -webkit-box-flex: 2;
  -webkit-flex: 2;
  flex: 2;
}

.yu-flex-item-thrice {
  -webkit-box-flex: 3;
  -webkit-flex: 3;
  flex: 3;
}

/* ==================
        grid布局
 ==================== */

.yu-grids {
  display: grid;
  grid-template-columns: repeat(3, 33.33%);
  grid-template-rows: repeat(3, 33.33%);
}

.yu-grid {
  text-align: center;
  background: #fff;
  padding: 20rpx 10rpx;
  box-sizing: border-box;
}

.yu-grid.yu-grid-border {
  border-right: 1rpx solid #d9d9d9;
  border-bottom: 1rpx solid #d9d9d9;
}

.yu-grids-float {
  width: 100%;
  overflow: hidden;
}

.yu-grids-float .yu-grid {
  float: left;
  width: 100%;
}

.yu-grids-float .yu-grid-col-2 {
  width: 50%;
}

.yu-grids-float .yu-grid-col-3 {
  width: 33.33%;
}

.yu-grids-float .yu-grid-col-4 {
  width: 25%;
}

.yu-grids-float .yu-grid-col-5 {
  width: 20%;
}

.yu-grids-flex {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  flex-wrap: wrap;
}

.yu-grids-flex .yu-grid {
  width: 100%;
}

.yu-grids-flex .yu-grid-col-2 {
  width: 50%;
}

.yu-grids-flex .yu-grid-col-3 {
  width: 33.33%;
}

.yu-grids-flex .yu-grid-col-4 {
  width: 25%;
}

.yu-grids-flex .yu-grid-col-5 {
  width: 20%;
}

/* ==================
        浮动布局
 ==================== */

.yu-float::after {
  clear: both;
  content: '';
  display: block;
  height: 0;
  visibility: hidden;
}

.yu-float-fl {
  float: left;
}

.yu-float-fr {
  float: right;
}

/* ==================
        单元格列表cell
 ==================== */

.yu-cells {
  background-color: #fff;
}

.yu-cell {
  background-color: #fff;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  padding: 24rpx 30rpx;
  font-size: 32rpx;
}

.yu-cell::before {
  content: " ";
  position: absolute;
  top: 0;
  right: 0;
  height: 1rpx;
  border-top: 1rpx solid #e5e5e5;
  color: #e5e5e5;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
  left: 30rpx;
  z-index: 2;
}

.yu-cells .yu-cell:first-child::before {
  display: none;
}

.yu-cell-hd .yu-cell-icon {
  font-size: 40rpx;
  color: #999;
  padding-right: 6rpx;
  width: 18px;
}
.yu-cell-hd{
 margin-right: 10rpx;
}
.yu-cell-hd .yu-cell-img {
  width: 36rpx;
}

.yu-cell-hd .yu-cell-md-img {
  width: 60rpx;
}

.yu-cell-hd .yu-cell-lg-img {
  width: 96rpx;
}

.yu-cell-bd {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.yu-cell-ft {
  text-align: right;
  color: #999;
  position: relative;
}

.yu-cells navigator:active {
  background: #f5f5f5;
}

.yu-cell-ft.yu-cell-icon {
  padding-right: 30rpx;
}

.yu-cell-ft.yu-cell-icon::after {
  content: '';
  display: inline-block;
  width: 8px;
  height: 8px;
  border-top: 1rpx solid #999;
  border-right: 1rpx solid #999;
  transform: rotate(45deg);
  -webkit-transform: rotate(45deg);
  vertical-align: middle;
  position: absolute;
  top: 50%;
  right: 2px;
  margin-top: -4px;
}

