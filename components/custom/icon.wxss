@font-face {
    font-family: 'custombar';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: normal;
    font-style: normal;
}

.icon{
  font-family: 'custombar' !important;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-home:before {
  content: "\e696";
}

.icon-return:before {
  content: "\e72c";
}

.icon-search:before {
  content: "\e745";
}

